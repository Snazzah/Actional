{
  "name": "actional",
  "version": "0.1.4",
  "description": "Let clients pick and choose what to send to each other with Socket.IO",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "author": "Snazzah",
  "repository": "https://github.com/Snazzah/Actional",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "lint": "npx eslint --ext .ts ./src",
    "lint:fix": "npx eslint --ext .ts ./src --fix",
    "gpr": "ts-node scripts/gpr",
    "prepublishOnly": "npm run lint:fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run build && git add lib"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  },
  "bin": {
    "actional-server": "./lib/cli.js"
  },
  "dependencies": {
    "@discordjs/collection": "^0.1.6",
    "abort-controller": "^3.0.0",
    "debug": "^4.2.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eventemitter3": "^4.0.7",
    "find-up": "^5.0.0",
    "prettier": "^2.2.1",
    "promise.allsettled": "^1.0.4",
    "socket.io": "^3.1.0",
    "socket.io-client": "^3.1.0",
    "yargs": "^16.0.3"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/node": "^14.14.22",
    "@types/promise.allsettled": "^1.0.3",
    "@types/yargs": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint": "^7.18.0",
    "husky": "^5.1.0",
    "lint-staged": "^10.5.3",
    "ts-node": "^9.1.1",
    "typescript": "4.1"
  }
}
